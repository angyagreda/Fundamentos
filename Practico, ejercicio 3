#include <iostream>

using namespace std;

int main()
{
    cout<<"Programa para calcular nota final"<<endl;

    float E, e1, e2, e3, e4, p, F;
    cout<<"Ingrese la nota de su 1° parcial: "<<endl;
    cin>> e1;
    cout<<"Ingrese la nota de su 2° parcial: "<<endl;
    cin>> e2;
    cout<<"Ingrese la nota de su 3° parcial: "<<endl;
    cin>> e3;
    cout<<"Ingrese la nota de su examen final: "<<endl;
    cin>> E;
    cout<<"Ingrese la nota de su proyecto final: "<<endl;
    cin>> p;

    e4=(e1+e2+e3)/3;
    F=((e4*40)/100)+((E*40)/100)+((p*20)/100);

    cout<<"La nota final es: ";
    cout<< F;


    return 0;
}

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

float AreaCubo(float a);

int main()
{
	float area, altura;
	cout<<"Ingrese la altura: "<<endl;
	cin>>altura;
	area= AreaCubo(altura);
	cout<<"El area es: "<<area;

	getch();

return 0;
}

float AreaCubo(float a)
{
	float area;
	area=6*a*a;
	return area;


}
